#!/usr/bin/env zsh

cd ${ZSH_CUSTOM:=~/.oh-my-zsh/custom}/plugins/fast-syntax-highlighting && git pull https://github.com/zdharma/fast-syntax-highlighting

cd ${ZSH_CUSTOM:=~/.oh-my-zsh/custom}/plugins/zsh-autosuggestions && git pull https://github.com/zsh-users/zsh-autosuggestions

cd ${ZSH_CUSTOM:=~/.oh-my-zsh/custom}/plugins/zsh-completions && git pull https://github.com/zsh-users/zsh-completions

cd ${ZSH_CUSTOM:=~/.oh-my-zsh/custom}/plugins/zsh-z && git pull https://github.com/agkozak/zsh-z

cd ${ZSH_CUSTOM:=~/.oh-my-zsh/custom}/themes/powerlevel10k && git pull --depth=1 https://github.com/romkatv/powerlevel10k.git
